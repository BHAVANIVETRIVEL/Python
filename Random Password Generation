# Python
import random
def generate_password(length):
"""Generates a random password of the specified length.
Agrs:
length: The length of the password to generate.
Returns: 
Returns a password of the specified length."""

# Create a lsit of all possible characters.
character = []
for i in range(ord('a'),ord('z')+1):
  characters.append(chr(i))
for i in range(ord('A'),ord('z')+1):
  characters.append(chr(i))
for i in range(ord('0'),ord('z')+1):
  characters.append(chr(i))
  Characters.append('!')
  Characters.append('@')
  Characters.append('$')
  Characters.append('%')
  Characters.append('^')
  Characters.append('&')
  Characters.append('*')
  Characters.append('(')
  Characters.append(')')
  Characters.append('-')
  Characters.append('_')
  Characters.append('+')
  Characters.append('=')

#Generate a random password.
password=''
for i in range(length):
  password += random.choice(characters)
return password

